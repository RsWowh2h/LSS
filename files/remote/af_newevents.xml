	<ItemSpec id="1002" name="AF_IM30_event3" lv_require="1" affair_timeMin="360" affair_timeMax="1440" last="1001"  />
	<ItemSpec id="1003" name="AF_IM30_event4" lv_require="1" affair_timeMin="360" affair_timeMax="1440" last="1002"  />
	</Group> 
	
	
	
	<Group id="af_timelimit">	
    <!-- 时间为注册多少天以内的用户有效。例：0为所有新老用户，7为7天内注册的新用户 -->		
	<ItemSpec id="1001" time="0" />
	</Group> 
	
	
